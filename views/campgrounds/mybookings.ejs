<% include ../partials/header %>


<div class="container">
  <table class="table table-striped table-bordered table-hover table-responsive box-shadow-table">
    <thead class="thead-inverse">
    <tr>
      <th>#</th>
      <th>Site Name</th>
      <th>Agency Name</th>
      <th>Email</th>
      <th>Mobile</th>
      <th>Address</th>
    </tr>
    </thead>
    <tbody>
      <% var a=1 %>
  <% campgrounds.forEach(function(campground){ %>

    <% campground.bookings.forEach(function(booking){ %>
      <% if(currentUser._id.equals(booking.author.id)){ %>
      <tr>
      <td><%= a++ %></td>
      <td><%= campground.name %></td>
      <td><%= campground.author.name %></td>
      <td><%= campground.author.username %></td>
      <td><%= campground.author.mobilenumber %></td>
      <td><%= campground.author.homeaddress %></td>
      </tr>
      <% } %>
    <% }) %>
  <% }) %>
    </tbody>
  </table>

</div>


<% include ../partials/footer %>
